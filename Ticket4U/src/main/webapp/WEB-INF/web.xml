<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <display-name>Nome_Della_Tua_App</display-name>

    <!-- Mappatura delle servlets -->
    <servlet>
        <servlet-name>AggiuntaEventoServlet</servlet-name>
        <servlet-class>controller.AggiuntaEventoServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AggiuntaEventoServlet</servlet-name>
        <url-pattern>/aggiuntaEvento</url-pattern>
    </servlet-mapping>


    <!-- Aggiungi qui le altre servlets e le loro mappature -->

    <!-- Mappatura delle pagine JSP -->
    <servlet>
        <servlet-name>index</servlet-name>
        <jsp-file>/index.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>AggiuntaNuovoEvento</servlet-name>
        <jsp-file>/WEB-INF/AggiuntaNuovoEvento.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>AggiuntaNuovoEvento</servlet-name>
        <url-pattern>/AggiuntaNuovoEvento.jsp</url-pattern>
    </servlet-mapping>

    <!-- Aggiungi qui le altre pagine JSP e le loro mappature -->

    <!-- Mappatura delle risorse statiche -->


</web-app>
